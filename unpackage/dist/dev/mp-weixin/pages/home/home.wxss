/* 水平间距 */
swiper {
  height: 330rpx;
}
swiper .swiper-item,
swiper image {
  width: 100%;
  height: 100%;
}
.nav-list {
  margin-top: 10rpx;
  display: flex;
  justify-content: space-around;
}
.nav-list .nav-img {
  width: 128rpx;
  height: 140rpx;
}
.floor-item {
  margin-top: 20rpx;
}
.floor-item .floor-title {
  width: 100%;
  height: 60rpx;
}
.floor-item .floor-img-box {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  grid-template-rows: repeat(2, 232rpx);
  padding: 0 10rpx;
  gap: 10rpx;
}
.floor-item .floor-img-box .floor-img > image {
  width: 100%;
  height: 100%;
}
.floor-item .floor-img-box .floor-img:nth-child(1) {
  grid-row: 1/3;
}